{
    "name": "koatty_store",
    "version": "1.4.6",
    "description": "Cache store for koatty.",
    "scripts": {
        "build": "del-cli --force dist && tsc",
        "eslint": "eslint --ext .ts,.js ./",
        "prepublishOnly": "npm test && npm run build",
        "release": "npm run build && standard-version",
        "test": "npm run eslint && jest --passWithNoTests",
        "test:cov": "jest --collectCoverage --detectOpenHandles",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s"
    },
    "main": "./dist/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/koatty/koatty_store.git"
    },
    "keywords": [
        "web",
        "typescript",
        "framework",
        "mvc",
        "koa2",
        "restful",
        "agile",
        "koatty_store",
        "koatty"
    ],
    "engines": {
        "node": ">10.0.0"
    },
    "author": {
        "name": "richenlin",
        "email": "richenlin@gmail.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/koatty/koatty_store/issues"
    },
    "homepage": "https://github.com/koatty/koatty_store",
    "maintainers": [
        {
            "name": "richenlin",
            "email": "richenlin@gmail.com"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.x.x",
        "@babel/plugin-proposal-decorators": "^7.x.x",
        "@babel/preset-env": "^7.x.x",
        "@babel/preset-typescript": "^7.x.x",
        "@commitlint/cli": "^12.x.x",
        "@commitlint/config-conventional": "^12.x.x",
        "@types/generic-pool": "^3.1.9",
        "@types/ioredis": "^4.26.4",
        "@types/jest": "^26.0.23",
        "@types/node": "^12.x.x",
        "@typescript-eslint/eslint-plugin": "^4.x.x",
        "@typescript-eslint/parser": "^4.x.x",
        "conventional-changelog-cli": "^2.x.x",
        "del-cli": "^3.x.x",
        "eslint": "^7.x.x",
        "eslint-plugin-jest": "^24.x.x",
        "husky": "^6.x.x",
        "jest": "^27.0.4",
        "jest-html-reporters": "^2.x.x",
        "koatty_cacheable": "^1.x.x",
        "koatty_schedule": "^1.x.x",
        "standard-version": "^9.x.x",
        "ts-jest": "^27.x.x",
        "ts-node": "^10.x.x",
        "typescript": "^4.x.x"
    },
    "dependencies": {
        "@outofsync/memory-cache": "^1.3.0",
        "generic-pool": "^3.7.8",
        "ioredis": "^4.27.6",
        "koatty_lib": "^1.x.x",
        "koatty_logger": "^1.x.x"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
